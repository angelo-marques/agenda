generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Agendamento{
    id Int                @id @default(autoincrement()) @map("id")
    title String          @map("title")
    dataInicial DateTime  @default(now()) @map("start_time")
    dataFinal DateTime    @map("end_time")
    @@map("appointments")
}